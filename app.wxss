page {
    line-height: 1.6;
    font-family: -apple-system-font,"Helvetica Neue",sans-serif;
    font-size: 28rpx;
    overflow-x: hidden;
    width: 100%;
}
icon {
    vertical-align: middle;
}
.widt{
  width: 95%;
  margin: 0 auto;
  overflow-x:hidden;
  overflow-y:hidden;
}
.clear{  clear: both;}
.bg_bs{background-color:#ffffff; }
.bg_ls{  background-color: #0078ff;}
.bg_cs{  background-color: #ff6634;}
.bg_hs{ background-color: #fafafa;}
.bg_bts{ background-color: #f2f2f2;}

.color_hei{color: #000000;}
.color_bai{  color: #ffffff;}
.color_hui{color: #4c4c4c;}
.color_danhui{color: #787878;}
.color_hong{color: #ff8c65;}

.font_18{  font-size: 38rpx;}
.font_16{  font-size: 34rpx;}
.font_14{  font-size: 30rpx;}
.font_12{  font-size: 26rpx;}
.font_10{  font-size: 22rpx;}

.height_10{ height: 10px}
/*文字超出省略*/
.ellipsis {
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    display: block;
}

/*对齐*/
.tl{text-align: left;}
.tc{text-align: center;}
.tr{text-align: right;}

/*边框*/
.bte{border-top:solid 1px #f2f2f2;}
.bbe{border-bottom: solid 1px #f2f2f2;}
.bre{ border-right: 1px solid #f2f2f2}
.ble{ border-left: 1px solid #f2f2f2}

/*浮动*/
.fl_l{ float: left;}
.fl_r{ float: right;}

.w10{ width: 10%;margin: 0 auto;}
