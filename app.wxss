.search{ width: 100%;border-radius: 35rpx; height: 70rpx; line-height: 70rpx;background-color:#ffffff;border:solid 1px #f2f2f2;}
.search .text_input{  line-height: 70rpx; height: 70rpx;   width: 95%; padding-left:5%; }
.search .submits{ width:40px;height:40px; z-index: 22;border: none;background: none; padding:0;} 
.search .submits::after{ border: none;} 
.search .submits image{ width:20px;height:20px; z-index: 33;} 
.button{line-height: 60rpx; height: 60rpx;  border-radius: 10rpx; }

.fotter{ 
  padding: 20px 0; line-height: 30px;display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center; 
}
.fotter image{ margin-right: 3px; width:20px; height: 20px; }