.top{ width:100%;height:150px; position: absolute;}
.top .logo{margin-top: 40px;}
.top .nickname{margin-top: 60px;}
.top .logo image{ width: 80px; height: 80px; border-radius: 40px;}
.tu{ width: 100%; height: 180px; overflow: hidden;}
.tu image{ width: 100%;}

 .nav {
    margin: 30px 0;
}

.nav .item {
    padding: 20rpx 0;
    color: #777;
    line-height: 8vw;
    height: 8vw;
}

.nav .item .fl_l image {
    width: 50rpx;
    height: 50rpx;
    border-radius: 25rpx;
    margin: 0 auto;
}
.nav .item .fl_r image {
    width: 30rpx;
    height: 30rpx;
    margin: 0 auto;
}
.ors{ box-shadow:2px 2px 3px 2px #f3f3f3; height: 45px; line-height: 45px;}
.ors .item{ height: 45px; line-height: 45px;}